  <xs:simpleType name="CalendarType">
    <xs:annotation>
      <xs:appinfo>
        <rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:adsic="http://www.adsic.abudhabi.ae/egms/1.0/" xmlns:dcterms="http://purl.org/dc/terms/" xmlns:mireg="http://mireg.org/schema/1.0/">
          <rdf:Description rdf:about="urn:ae:gov:abudhabi:core:CommonDataTypes:1/1.2#CalendarType">
            <dcterms:title xml:lang="ar">التقويم</dcterms:title>
            <dcterms:title xml:lang="en">Calendar</dcterms:title>
            <dcterms:description xml:lang="ar">يتم استخدامها للاشارة الى استعمال التاريخ للتقويم الهجري أو الميلادي</dcterms:description>
            <dcterms:description xml:lang="en">Used to indicate whether a date uses the Gregorian or Higri calendar.</dcterms:description>
            <mireg:owner rdf:datatype="http://adsic.abudhabi.ae/egms/1.0/encodingschemes#contact">
              <adsic:OrganisationName xml:lang="en">ADSIC مركز أبو ظبي للانظمة الالكترونية والمعلومات</adsic:OrganisationName>
            </mireg:owner>
            <mireg:version>1.1</mireg:version>
            <dcterms:created>2009-06-05</dcterms:created>
            <dcterms:creator rdf:datatype="http://adsic.abudhabi.ae/egms/1.0/encodingschemes#contact">
              <adsic:OrganisationName xml:lang="en">ADSIC</adsic:OrganisationName>
            </dcterms:creator>
          </rdf:Description>
        </rdf:RDF>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="Gregorian" />
      <xs:enumeration value="Higri" />
    </xs:restriction>
  </xs:simpleType>
