  <xs:simpleType name="NumericIdentifierType">
    <xs:annotation>
      <xs:appinfo>
        <rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:adsic="http://www.adsic.abudhabi.ae/egms/1.0/" xmlns:dcterms="http://purl.org/dc/terms/" xmlns:mireg="http://mireg.org/schema/1.0/">
          <rdf:Description rdf:about="urn:ae:gov:abudhabi:core:CommonDataTypes:1/1.2#NumericIdentifierType">
            <dcterms:title xml:lang="ar">المعرف الرقمي</dcterms:title>
            <dcterms:title xml:lang="en">Numeric Identifier</dcterms:title>
            <dcterms:description xml:lang="ar">سلسلة يمكن أن تحتوي فقط الارقام 0-9 .والمنوي استخدامها للمعرفين
                                               العدديين،وبالامكان الحد من طولها</dcterms:description>
            <dcterms:description xml:lang="en">A string that can only have the digits 0-9. It is intended for use for
                                               numeric identifiers, and can have its length restricted.</dcterms:description>
            <mireg:owner rdf:datatype="http://adsic.abudhabi.ae/egms/1.0/encodingschemes#contact">
              <adsic:OrganisationName xml:lang="en">ADSIC مركز أبو ظبي للانظمة الالكترونية والمعلومات</adsic:OrganisationName>
            </mireg:owner>
            <mireg:version>1.1</mireg:version>
            <dcterms:created>2009-06-08</dcterms:created>
            <dcterms:creator rdf:datatype="http://adsic.abudhabi.ae/egms/1.0/encodingschemes#contact">
              <adsic:OrganisationName xml:lang="en">ADSIC</adsic:OrganisationName>
            </dcterms:creator>
          </rdf:Description>
        </rdf:RDF>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="[0-9]*" />
    </xs:restriction>
  </xs:simpleType>
