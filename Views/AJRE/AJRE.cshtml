@section Scripts {

    <script src="~/Scripts/bootstrap.min.js"></script>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/bootstrap-theme.min.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
    <script src="~/Scripts/jquery-ui.js"></script>
    <link href="~/Content/jquery-ui.css" rel="stylesheet" />
    <link href="~/Content/jquery-ui.theme.css" rel="stylesheet" />
    <link href="~/Content/jquery-ui.structure.css" rel="stylesheet" />
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jspdf/1.3.5/jspdf.min.js"></script>
    <script src="https://html2canvas.hertzen.com/dist/html2canvas.js"></script>
    <script src="~/Scripts/GeneratePDF.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-browser/0.1.0/jquery.browser.min.js"></script>

    <script>



        $(document).ready(function () {

            $('.DataTable').hide();

        });


         function getData() {

            $("#divLoading").html('<image src="../Content/images/ajax-loader.gif" alt="Loading, please wait" />');

            var url = '@Url.Action("Search","AJRE")';

             $(".tbody").empty();
             $(".tbody1").empty();
             $(".tbody2").empty();
            $('.DataTable').hide();
            document.getElementById('response').innerText = ('');
             var html = '';
             var html1 = '';
             var html2 = '';

            $('#error').fadeIn();
            $('#response').fadeIn();
            if ($("#txtEmiratesId").val().length == 0) {

                document.getElementById('error').innerText = ('Invalid ID Type');
                document.getElementById('error').style.color = 'white';

                $("#txtEmiratesId").focus();
                $("#divLoading").html("");

                return false;
            }
            else {
                document.getElementById('error').innerText = ('');
            }

             var input = { "EmiratesID": $('#txtEmiratesId').val() };
              var UsrAgent = { "UserAgent": $.browser };

            $.get(url, { postdata: JSON.stringify(input), UserAgent: JSON.stringify(UsrAgent)  }, function (data) {
                if (data == '') {
                    $("#divLoading").html("");
                    return;
                }

                var obj = JSON.parse(data);
                if (obj.flag == "1") {

                    $.each(obj.objOwnerInfo, function (key, item) {

                        html += '<tr>';
                        html += '<td>' + obj.objOwnerInfo.OwnerNameAR + '</td>';
                        html += '<td>' + obj.objOwnerInfo.OwnerNameEN + '</td>';
                        html += '<td>' + obj.objOwnerInfo.OwnerNationalityCode + '</td>';
                        html += '<td>' + obj.objOwnerInfo.OwnerIdentityId + '</td>';
                        html += '</tr>';


                        if (obj.objOwnerInfo.UnitsInfoList.length > 0) {
                            $.each(obj.objOwnerInfo.UnitsInfoList, function (key, item1) {
                                html1 += '<tr>';
                                html1 += '<td>' + item1.UnitPropertyId + '</td>';
                                html1 += '<td>' + item1.UnitProjectId + '</td>';
                                html1 += '<td>' + item1.UnitMainProjectNameAR + '</td>';
                                html1 += '<td>' + item1.UnitMainProjectNameEN + '</td>';
                                html1 += '<td>' + item1.UnitMainProjectNameAR + '</td>';
                                html1 += '<td>' + item1.UnitCreatedAt + '</td>';
                                html1 += '<td>' + item1.UnitShare + '</td>';
                                html1 += '</tr>';
                            });
                        }
                        if (obj.objOwnerInfo.LandsInfoList.length > 0) {
                            $.each(obj.objOwnerInfo.LandsInfoList, function (key, item2) {
                                html2 += '<tr>';
                                html2 += '<td>' + item2.LandDeedId + '</td>';
                                html2 += '<td>' + item2.LandCreatedAt + '</td>';
                                html2 += '<td>' + item2.LandOwnershipType + '</td>';
                                html2 += '<td>' + item2.LandShare + '</td>';
                                html2 += '<td>' + item2.LandId + '</td>';
                                html2 += '<td>' + item2.LandCityAR + '</td>';
                                html2 += '<td>' + item2.LandSectorAR + '</td>';
                                html2 += '<td>' + item2.LandDistrictAR + '</td>';
                                html2 += '</tr>';
                            });
                        }


                    });

                    $(".tbody").append(html);
                    $(".tbody1").append(html1);
                    $(".tbody2").append(html2);
                    $('.DataTable').show();
                }
                else {
                    if (obj.flag == "2"  ) {
                        document.getElementById('response').innerText = (obj.ResponseDescription);
                        document.getElementById('response').style.color = 'white';
                    }
                    else if(obj.flag == "3"  ) {
                        document.getElementById('response').innerText = (obj.ResponseDescription);
                        document.getElementById('response').style.color = 'white';
                    }

                }
                $("#divLoading").html("");
            });
        }

        $("#btnsubmit").on("click", getData);

         $('#create_pdf').on('click', function () {
            getPDF("AJRE.pdf");
       });

    </script>
