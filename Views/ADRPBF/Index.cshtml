         function getData() {

            $("#divLoading").html('<image src="../Content/images/ajax-loader.gif" alt="Loading, please wait" />');

            var url = '@Url.Action("Search","ADRPBF")';
             var id = "2";
             $(".tbody").empty();
             $(".tbody1").empty();
             $(".tbody2").empty();
             $(".tbody3").empty();
             $(".tbody4").empty();
             $(".tbody5").empty();
             $(".tbody6").empty();
             $(".tbody7").empty();
             $(".tbody8").empty();


             $('.DataTable').hide();
             $('.DataTable1').hide();

            document.getElementById('response').innerText = ('');
            var html = '';
             var html1 = '';
             var html2 = '';
             var html3 = '';
             var html4 = '';
             var html5 = '';
             var html6 = '';
             var html7 = '';
             var html8 = '';
             var html9 = '';


            $('#error').fadeIn();
            $('#response').fadeIn();
            if (id == 2 && $("#txtEmiratesId").val().length == 0) {

                document.getElementById('error').innerText = ('Invalid ID Type');
                document.getElementById('error').style.color = 'white';

                $("#txtEmiratesId").focus();


                 $("#divLoading").html("");

                return false;
             }
            else if (id == 3 && $("#txtTNo").val().length == 0 && $("#txtFNo").val().length == 0) {

                document.getElementById('error').innerText = ('Town Number is mandatory');
                document.getElementById('error').style.color = 'white';

                $("#txtTNo").focus();


                $("#divLoading").html("");

                return false;
            }
            else if (id == 3 && $("#txtFNo").val().length == 0) {

                document.getElementById('error').innerText = ('Family Number is mandatory');
                document.getElementById('error').style.color = 'white';

                $("#txtFNo").focus();


                $("#divLoading").html("");

                return false;
            }
            else {
                document.getElementById('error').innerText = ('');
            }

             var input = { "EmiratesID": $('#txtEmiratesId').val(), "TNo": $("#txtTNo").val(), "FNo": $('#txtFNo').val() };
             var UsrAgent = { "UserAgent": $.browser };

            $.get(url, { index: "2", postdata: JSON.stringify(input), UserAgent: JSON.stringify(UsrAgent) }, function (data) {
                if (data == '') {
                    $("#divLoading").html("");
                    return;
                }

                var obj = JSON.parse(data);

                if (obj.flag == "1") {
                    if (obj.memberInformationResponse != null) {
                        html6 += '<tr>';
                        html6 += '<td>' + obj.memberInformationResponse.Address + '</td>';
                        html6 += '<td>' + obj.memberInformationResponse.City + '</td>';
                        html6 += '<td>' + obj.memberInformationResponse.DOB + '</td>';
                        html6 += '<td>' + obj.memberInformationResponse.Email + '</td>';
                        html6 += '<td>' + obj.memberInformationResponse.Emirate + '</td>';
                        html6 += '<td>' + obj.memberInformationResponse.FamilyBookNo + '</td>';
                        html6 += '</tr>';

                        html7 += '<tr>';
                        html7 += '<td>' + obj.memberInformationResponse.BeneficiaryInformation + '</td>';
                        html7 += '<td>' + obj.memberInformationResponse.MemberFullNameArabic + '</td>';
                        html7 += '<td>' + obj.memberInformationResponse.MemberFullNameEnglish + '</td>';
                        html7 += '<td>' + obj.memberInformationResponse.MemberType + '</td>';
                        html7 += '<td>' + obj.memberInformationResponse.Mobile + '</td>';
                        html7 += '<td>' + obj.memberInformationResponse.NationalID + '</td>';
                        html7 += '</tr>';

                        html8 += '<tr>';
                        html8 += '<td>' + obj.memberInformationResponse.POBOX + '</td>';
                        html8 += '<td>' + obj.memberInformationResponse.PensionAmount + '</td>';
                        html8 += '<td>' + obj.memberInformationResponse.Fax + '</td>';
                        html8 += '<td>' + obj.memberInformationResponse.PensionNumber + '</td>';
                        html8 += '<td>' + obj.memberInformationResponse.PensionStartDate + '</td>';
                        html8 += '<td>' + obj.memberInformationResponse.Phone + '</td>';
                        html8 += '</tr>';




                        $(".tbody6").append(html6);
                        $(".tbody7").append(html7);
                        $(".tbody8").append(html8);


                        $('.DataTable1').show();
                    }

                }
               else if (obj.flag == "2") {

                    if (obj.aDRPBFDetailsEmiratedId != null) {

                            html += '<tr>';
                            html += '<td>' + obj.aDRPBFDetailsEmiratedId.ActiveStartDate + '</td>';
                            html += '<td>' + obj.aDRPBFDetailsEmiratedId.ActiveemployerNameAr + '</td>';
                            html += '<td>' + obj.aDRPBFDetailsEmiratedId.ActiveemployerNameEn + '</td>';
                            html += '<td>' + obj.aDRPBFDetailsEmiratedId.ActivejobTitle + '</td>';
                            html += '<td>' + obj.aDRPBFDetailsEmiratedId.Activesalary + '</td>';
                            html += '<td>' + obj.aDRPBFDetailsEmiratedId.Address + '</td>';

                            html += '</tr>';


                            html1 += '<tr>';
                            html1 += '<td>' + obj.aDRPBFDetailsEmiratedId.City + '</td>';
                            html1 += '<td>' + obj.aDRPBFDetailsEmiratedId.DOB + '</td>';
                            html1 += '<td>' + obj.aDRPBFDetailsEmiratedId.Email + '</td>';
                            html1 += '<td>' + obj.aDRPBFDetailsEmiratedId.Emirate + '</td>';
                            html1 += '<td>' + obj.aDRPBFDetailsEmiratedId.EmployerSector + '</td>';
                            html1 += '<td>' + obj.aDRPBFDetailsEmiratedId.FamilyBookCityID + '</td>';
                            html1 += '</tr>';

                            html2 += '<tr>';
