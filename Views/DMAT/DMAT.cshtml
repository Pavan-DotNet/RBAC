    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
    <script src="~/Scripts/jquery-ui.js"></script>
    <link href="~/Content/jquery-ui.css" rel="stylesheet" />
    <link href="~/Content/jquery-ui.theme.css" rel="stylesheet" />
    <link href="~/Content/jquery-ui.structure.css" rel="stylesheet" />
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jspdf/1.3.5/jspdf.min.js"></script>
    <script src="https://html2canvas.hertzen.com/dist/html2canvas.js"></script>
    <script src="~/Scripts/GeneratePDF.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-browser/0.1.0/jquery.browser.min.js"></script>

    <script>



        $(document).ready(function () {

            $('.DataTable').hide();

        });


         function getData() {

            $("#divLoading").html('<image src="../Content/images/ajax-loader.gif" alt="Loading, please wait" />');

            var url = '@Url.Action("Search","DMAT")';

             $(".tbody").empty();
             $(".tbody1").empty();
             $(".tbody2").empty();
             $(".tbody3").empty();
             $(".tbody4").empty();
             $(".tbody5").empty();
             $(".tbody6").empty();
             $(".tbody7").empty();
             $(".tbody8").empty();
             $(".tbody9").empty();
             $(".tbody10").empty();

            $('.DataTable').hide();
            document.getElementById('response').innerText = ('');
             var html = '';
             var html1 = '';
             var html2 = '';
             var html3 = '';
             var html4 = '';
             var html5 = '';
             var html6 = '';
             var html7 = '';
             var html8 = '';
             var html9 = '';
             var html10 = '';

            $('#error').fadeIn();
            $('#response').fadeIn();
            if ($("#txtEmiratesId").val().length == 0) {

                document.getElementById('error').innerText = ('Invalid ID Type');
                document.getElementById('error').style.color = 'white';

                $("#txtEmiratesId").focus();


                 $("#divLoading").html("");

                return false;
            }
            else {
                document.getElementById('error').innerText = ('');
            }

           var input={"EmiratesID":  $('#txtEmiratesId').val() };
           var UsrAgent = { "UserAgent": $.browser };
            $.get(url, { postdata: JSON.stringify(input), UserAgent: JSON.stringify(UsrAgent)  }, function (data) {
                if (data == '') {
                    $("#divLoading").html("");
                    return;
                }

                var obj = JSON.parse(data);
                if (obj.flag == "1") {
                    for (var i = 0; i < obj.listPlot.length; i++) {
                        html += '<tr>';

                        html += '<td>' + StringHasValue(obj.listPlot[i].plotId) + '</td>';
                        html += '<td>' + StringHasValue(obj.listPlot[i].municipalityId) + '</td>';
                        html += '<td>' + StringHasValue(obj.listPlot[i].municipalityNameA) + '</td>';
                        html += '<td>' + StringHasValue(obj.listPlot[i].municipalityNameE) + '</td>';
                        html += '<td>' + StringHasValue(obj.listPlot[i].districtId) + '</td>';
                        html += '<td>' + StringHasValue(obj.listPlot[i].districtNameA) + '</td>';
                        html += '<td>' + StringHasValue(obj.listPlot[i].districtNameE) + '</td>';
                        html += '</tr>';

                        html1 += '<tr>';
                        html1 += '<td>' + StringHasValue(obj.listPlot[i].districtNumber) + '</td>';
                        html1 += '<td>' + StringHasValue(obj.listPlot[i].communityId) + '</td>';
                        html1 += '<td>' + StringHasValue(obj.listPlot[i].communityNameA) + '</td>';
                        html1 += '<td>' + StringHasValue(obj.listPlot[i].communityNameE) + '</td>';
                        html1 += '<td>' + StringHasValue(obj.listPlot[i].communityNumber) + '</td>';
                        html1 += '<td>' + StringHasValue(obj.listPlot[i].roadId) + '</td>';
                        html1 += '<td>' + StringHasValue(obj.listPlot[i].roadNumber) + '</td>';
                        html1 += '</tr>';

                        html2 += '<tr>';
                        html2 += '<td>' + StringHasValue(obj.listPlot[i].roadNameA) + '</td>';
                        html2 += '<td>' + StringHasValue(obj.listPlot[i].roadNameE) + '</td>';
